{
  "name": "oat-sa/lib-em-php-lti-client",
  "type": "library",
  "license": "GPL-2.0-only",
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/oat-sa/lib-em-php-lti-events"
    }
  ],
  "require": {
    "php": ">=7.4",
    "oat-sa/lib-em-php-lti-events": "*",
    "psr/http-message": "^1.0",
    "guzzlehttp/guzzle": "^6.4|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "OAT\\Library\\EnvironmentManagementLtiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OAT\\Library\\EnvironmentManagementLtiClient\\Tests\\": "tests/"
    }
  }
}
